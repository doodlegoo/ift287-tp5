<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN" "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">
<web-app id="test-biblio">
  <display-name>Système de gestion de ligue baseball</display-name>
  <context-param>
    <param-name>exempleParam1</param-name>
    <param-value>valeurDeExempleParam1</param-value>
  </context-param>
  <context-param>
    <param-name>exempleParam2</param-name>
    <param-value>valeurDeExempleParam2</param-value>
  </context-param>
  <listener>
    <listener-class>ligueBaseballServlet.BaseballSessionListener</listener-class>
    <listener-class>ligueBaseballServlet.BaseballContextListener</listener-class>
  </listener>
  <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>ligueBaseballServlet.Login</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Test</servlet-name>
    <servlet-class>ligueBaseballServlet.Test</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Logout</servlet-name>
    <servlet-class>biblioServlet.Logout</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SelectionMembre</servlet-name>
    <servlet-class>biblioServlet.SelectionMembre</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ListePretMembre</servlet-name>
    <servlet-class>biblioServlet.ListePretMembre</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Emprunt</servlet-name>
    <servlet-class>biblioServlet.Emprunt</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/Login</url-pattern>
    <url-pattern>/</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Test</servlet-name>
    <url-pattern>/Test</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Logout</servlet-name>
    <url-pattern>/Logout</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SelectionMembre</servlet-name>
    <url-pattern>/SelectionMembre</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ListePretMembre</servlet-name>
    <url-pattern>/ListePretMembre</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Emprunt</servlet-name>
    <url-pattern>/Emprunt</url-pattern>
  </servlet-mapping>
</web-app>
